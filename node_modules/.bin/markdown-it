#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules/markdown-it/bin/node_modules:/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules/markdown-it/node_modules:/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules:/mnt/c/Users/paddy/Code/node_modules:/mnt/c/Users/paddy/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules/markdown-it/bin/node_modules:/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules/markdown-it/node_modules:/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules:/mnt/c/Users/paddy/Code/node_modules:/mnt/c/Users/paddy/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/paddy/Code/bolt-generated-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../markdown-it/bin/markdown-it.js" "$@"
fi
